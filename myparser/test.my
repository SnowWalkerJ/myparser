x = 3;

function power(base, p) {
    if (p == 1) {
        return base;
    } else {
        return base * power(base, p - 1);
    }
}

function power2(base, p) {
    result = 1;
    while (p > 0) {
        result = result * base;
        p = p - 1;
    }
    print result;
    return 1;
}

